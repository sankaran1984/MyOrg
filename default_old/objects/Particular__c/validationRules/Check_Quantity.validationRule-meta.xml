<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Quantity</fullName>
    <active>true</active>
    <errorConditionFormula>(Quantity__c &lt;= 0) || (Quantity__c &gt; Stock__r.Quantity__c )</errorConditionFormula>
    <errorDisplayField>Quantity__c</errorDisplayField>
    <errorMessage>The quantiyt cannont be zero OR more than in stock</errorMessage>
</ValidationRule>
